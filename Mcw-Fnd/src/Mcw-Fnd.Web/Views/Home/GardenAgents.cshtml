@model Mcw_Fnd.Web.ViewModels.GardenAgentsViewModel

@{
    ViewData["Title"] = "Top-10 Real Estate Agents for houses with garden";
}

<h2>@ViewData["Title"].</h2>



<div class="panel panel-primary" style="margin-top: 50px">
    <div class="panel-heading">
        <div class="row">
            <div class="col-md-5">
                <h3 class="panel-title">Real Estate Agent</h3>
            </div>
            <div class="col-md-4">
                <h3 class="panel-title">Amount of houses for sale</h3>
            </div>
        </div>
    </div>

    <div class="panel-body">
        @foreach (var agent in Model.TopGardenAgentsOrdered)
        {
            <div class="row">
                <div class="col-md-5">
                    <label>@agent.Name</label>
                </div>
                <div class="col-md-4">
                    <label>@agent.AmountOfHousesForSale</label>
                </div>
            </div>
        }
    </div>

    <div class="panel-footer text-right">Results are cached for 300 seconds.</div>
</div>